        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Directional input state
up= 0;
down= 0;
left= 0;
right= 0;

// Quit timer (holding ESC)
quit= 0;

// Raw input flags before processing
try_up= 0;
try_down= 0;
try_left= 0;
try_right= 0;

canquit= 1; // Can the game be quit via ESC?
h_skip= 0;  // Text skipping flag

// Joystick axis variables
j_xpos= 0;
j_ypos= 0;
j_dir= 0;
j_fr= 0;
j_fl= 0;
j_fu= 0;
j_fd= 0;
j_fr_p= 0;
j_fl_p= 0;
j_fu_p= 0;
j_fd_p= 0;
i= 0;
while(i &lt; 12) {
    j_prev[i]= 0;
    j_on[i]= 0;
    i++;
}
// Default joystick button mappings
global.button0= 2; // Z / Confirm
global.button1= 1; // X / Cancel
global.button2= 4; // C / Menu

// Analog stick sensitivity settings
global.analog_sense= 0.15;
global.analog_sense_sense= 0.01;
global.joy_dir= 0;

// Load joystick config from ini
ini_open("config.ini");
b0_i= ini_read_real("joypad1", "b0", -1);
b1_i= ini_read_real("joypad1", "b1", -1);
b2_i= ini_read_real("joypad1", "b2", -1);
as_i= ini_read_real("joypad1", "as", -1);
jd_i= ini_read_real("joypad1", "jd", -1);

// Apply loaded config if valid
if(b0_i &gt;= 0) global.button0= b0_i;
if(b1_i &gt;= 0) global.button1= b1_i;
if(b2_i &gt;= 0) global.button2= b2_i;
if(as_i &gt;= 0) global.analog_sense= as_i;
if(jd_i &gt;= 0) global.joy_dir= jd_i;
ini_close();

debug_r= 0;
debug_f= 0;
j1= 0;
j2= 0;
ja= 0;
j_ch= 0;
jt= 0;
spec_rtimer= 0;
</string>
          </argument>
        </arguments>
      </action>
